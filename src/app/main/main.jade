.container
  .row
    .large-3.columns
      app-fdr-side-bar
    .large-9.columns
      app-fdr-product(
        class=[
          'large-3',
          'columns'
        ]
        ng-repeat="product in main.products | filter: product.seriesId | filter: product.prodTypeId track by $index"
        app-fdr-product-series="product.series"
        app-fdr-product-type="product.productType"
        app-fdr-product-img="product.images[1]"
      )